
@{
    Layout = null;
}
@using MVC_Basics.Common;
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <form>
        <div>
            <h2>Index View Containt</h2>
            @Html.ActionLink("Go to Student Creation Form", "Create")<br />
            Name : <input type="Text" id="txtName" name="txtName" />
            <br />
            Profile Pic :<img src="data:image/jpeg;base64," alt="Rohit Sharma" />
            <br />
            Password :<input type="Password" id="txtPassword" name="txtPassword" /> <br />
            Gendar   :<input type="radio" />Male <input type="radio" />Female<br />
            <a href="http://www.google.com">Visit google.com</a><br />
            <label>Address :</label>
            <textarea rows="5" cols="70"></textarea><br />

            <h2>HTML Helper Class</h2>
            Name : @Html.TextBox("htmlName")
            <br />
            Password : @Html.Password("htmlPassword")
            <br />
            @Html.ActionLink("Visit Welcome Method", "Welcome", "Home")
            <br />
            @Html.Label("Address :")
            @Html.TextArea("htmlAddress", new { rows = 5, cols = 70 })
            <br />
            Profile Pic :
            @Html.Image("data:image/jpeg;base64,")
            <br />
            @Html.Label("Gender")
            @Html.RadioButton("Male", "Male")Male
            @Html.RadioButton("Female", "Female")Female

        </div>
    </form>
</body>
</html>
